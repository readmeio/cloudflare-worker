{
  "name": "@readme/cloudflare-worker",
  "description": "Cloudflare worker to get your API log data into ReadMe",
  "version": "1.1.1",
  "dependencies": {
    "minimatch": "^3.0.4"
  },
  "engines": {
    "node": "^12 || ^14 || ^16",
    "npm": "^7"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different \"./**/**.js\"",
    "test": "nyc --reporter=lcov mocha",
    "prepublishOnly": "webpack --host https://metrics.readme.io",
    "posttest": "nyc check-coverage --statements 90 --branches 90 --functions 90 --lines 90"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org",
    "access": "public"
  },
  "author": "Dom Harrington <dom@readme.io>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/cloudflare-worker.git"
  },
  "devDependencies": {
    "@readme/eslint-config": "^7.1.1",
    "eslint": "^7.32.0",
    "mocha": "^5.2.0",
    "module-alias": "^2.1.0",
    "nock": "^10.0.0",
    "node-fetch": "^2.2.0",
    "nyc": "^13.0.1",
    "prettier": "^2.4.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "prettier": "@readme/eslint-config/prettier"
}
